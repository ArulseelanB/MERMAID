{
  "name": "@mermaid-js/mermaid-flowchart-v3",
  "version": "9.4.0",
  "description": "An extension for the Mermaid diagramming library that utilizes elkjs for layout, enabling the creation of visually appealing and easy-to-understand flowcharts.",
  "module": "dist/mermaid-flowchart-v3.core.mjs",
  "types": "dist/detector.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/mermaid-flowchart-v3.core.mjs",
      "types": "./dist/detector.d.ts"
    },
    "./*": "./*"
  },
  "keywords": [
    "diagram",
    "markdown",
    "flowchart",
    "mermaid",
    "elkjs"
  ],
  "scripts": {
    "prepublishOnly": "pnpm -w run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mermaid-js/mermaid"
  },
  "author": "Knut Sveidqvist",
  "license": "MIT",
  "standard": {
    "ignore": [
      "**/parser/*.js",
      "dist/**/*.js",
      "cypress/**/*.js"
    ],
    "globals": [
      "page"
    ]
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.0",
    "graphlib": "^2.1.0",
    "dagre-d3-es": "7.0.6",
    "elkjs": "^0.8.2",
    "d3": "^7.0.0",
    "khroma": "^2.0.0",
    "non-layered-tidy-tree-layout": "^2.0.2"
  },
  "devDependencies": {
    "concurrently": "^7.5.0",
    "mermaid": "workspace:*",
    "rimraf": "^3.0.2"
  },
  "resolutions": {
    "d3": "^7.0.0"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ]
}
